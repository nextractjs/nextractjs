{
  "compilerOptions": {
    "strictNullChecks": true,
    "target": "ES6",
    "allowJs": true,
    "strict": true,
    "importHelpers": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "resolveJsonModule": true,
    "jsx": "preserve",
    "declaration": true,
    "skipLibCheck": true
  }
}
