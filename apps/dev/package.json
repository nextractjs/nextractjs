    "autoprefixer": "^10.4.4",
    "next": "^12.1.6",
    "postcss": "^8.4.12",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwindcss": "^3.0.23"
  },
  "devDependencies": {
    "@types/node": "17.0.23",
    "@types/react": "17.0.42",
    "eslint-config-next": "12.1.0",
    "typescript": "4.6.2",
    "prisma": "^3.12.0"
  }
}
